{
	"parserOptions": {
		"ecmaVersion": 2017
	},
	"settings": {
		"react": {
			"version": "999.999.999"
		}
	},
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true,
		"jquery": true
	},
	"extends": "airbnb",
	"rules": {
		"no-duplicate-case": "error",
		"no-empty": "error",
		"no-extra-semi": "error",
		"no-func-assign": "error",
		"no-irregular-whitespace": "error",
		"no-unreachable": "error",
		"curly": "error",
		"dot-notation": "error",
		"eqeqeq": "error",
		"no-empty-function": "error",
		"no-multi-spaces": "error",
		"no-mixed-spaces-and-tabs": "error",
		"no-trailing-spaces": "error",
		"default-case": "error",
		"no-fallthrough": "error",
		"no-unused-vars": "error",
		"no-use-before-define": "error",
		"no-redeclare": "error",
		"camelcase": "error",
		"brace-style": "error",
		"consistent-return": "off",
		"indent": [
			2,
			"tab"
		],
		"no-tabs": 0,
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		]
	}
}